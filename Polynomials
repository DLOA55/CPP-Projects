	void Polynomial::calculate()
	{
	for(vector<value_type>::size_type i=poly1.size()-1 ; i >=0 ;--i)
	{
		int index=i;
		(while index>poly2.size()-1)
		{
		index-=1;
		}
		(while index<poly2.size()-1)
		{
		index+=1;
		}
		int ending_poly= poly1[i] operator poly2[index] 
		stack.push(ending_poly);
	}	
	cout << "The new Polynomials Coefficcients are: ";
	for(stack<value_type>::size_type i=1; i<stack.size(); ++i)
	{
		cout << stack.pop();
	}
	}
}
